{
  "name": "portfolio-site",
  "version": "1.0.0",
  "description": "Portfolio site with Next.js frontend",
  "private": true,
  "workspaces": [
    "src-code"
  ],
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd src-code && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd src-code && npm run build",
    "start": "npm run start:frontend",
    "start:frontend": "cd src-code && npm run start",
    "test": "npm run test:frontend",
    "test:frontend": "cd src-code && npm run test",
    "test:watch": "npm run test:watch:frontend",
    "test:watch:frontend": "cd src-code && npm run test:watch",
    "test:coverage": "npm run test:coverage:frontend",
    "test:coverage:frontend": "cd src-code && npm run test:coverage",
    "lint": "npm run lint:frontend",
    "lint:frontend": "cd src-code && npm run lint",
    "lint:fix": "npm run lint:fix:frontend",
    "lint:fix:frontend": "cd src-code && npm run lint",
    "type-check": "cd src-code && npm run type-check",
    "install:all": "npm install && cd src-code && npm install",
    "clean": "npm run clean:frontend",
    "clean:frontend": "cd src-code && rimraf .next node_modules"
  },
  "devDependencies": {
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "portfolio",
    "nextjs"
  ],
  "author": "Your Name",
  "license": "MIT"
}
